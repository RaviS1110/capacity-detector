{
    "model_id": "alwaysai/mobilenet_ssd",
    "mode": "camera",
    "camera_id": 0,
    "device_id": 1,
    "filename": "<.mov_or_.mp4_file>",
    "server_entry_url": "http://localhost:8888/new_entry",
    "server_exit_url": "http://localhost:8888/new_exit",
    "confidence": 0.3,
    "enable_logs": true,
    "enable_streamer": true,
    "deregister_frames": 30,
    "max_distance": 100,
    "target_labels": [
        "person"
    ],
    "entry_zones": [
        {
            "threshold": 0.1,
            "start_x": 0,
            "start_y": 0,
            "end_x": 300,
            "end_y": 720
        }
    ],
    "exit_zones": [
        {
            "threshold": 0.1,
            "start_x": 800,
            "start_y": 0,
            "end_x": 1280,
            "end_y": 720
        }
    ]
}